{
  "name": "capstone-back-end",
  "version": "1.0.0",
  "description": "Capstone project back end",
  "scripts": {
    "start": "node dist/index.js",
    "start:bun": "bun run src/index.ts",
    "dev:bun": "bun run --watch src/index.ts",
    "dev:node": "nodemon",
    "build:node": "npx tsc",
    "build:bun": "bunx tsc",
    "migration:generate": "drizzle-kit generate",
    "migrate:bun": "bun src/drizzle/migration.ts",
    "migrate:node": "npx tsx src/drizzle/migration.ts",
    "seed:bun": "bun src/drizzle/seeder.ts",
    "seed:node": "npx tsx src/drizzle/seeder.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Capstone-Buddies/app-back-end.git"
  },
  "keywords": [
    "back-end",
    "Bangkit Academy 2024",
    "capstone project",
    "Express.js"
  ],
  "author": "Capstone Buddies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Capstone-Buddies/app-back-end/issues"
  },
  "homepage": "https://github.com/Xrayya/express-playground#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "drizzle-kit": "^0.21.4",
    "nodemon": "^3.1.1",
    "tsx": "^4.11.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.2",
    "@types/express": "^4.17.21",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.8",
    "nanoid": "3.x.x",
    "util": "^0.12.5",
    "zod": "^3.23.8"
  }
}
